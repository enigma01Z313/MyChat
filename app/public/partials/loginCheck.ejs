<script>
(function() {
  const loggedUser = JSON.parse( localStorage.getItem('userData') )
  const {status: {code}, publicLock} = loggedUser

  if(
    window.location.href !== 'http://localhost:30000/setup' && 
    (code === 0 || publicLock === null))
    return window.location.replace(`http://localhost:30000/setup`);
})()
</script>